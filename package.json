    "codelyzer": "0.0.23",
    "json-loader": "^0.5.4",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "tslint-loader": "^2.1.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
